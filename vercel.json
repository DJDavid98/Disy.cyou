{
  "rewrites": [
    {
      "source": "/s/:match*",
      "destination": "https://i.disy.cyou/:match*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "interest-cohort=()"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        }
      ]
    }
  ],
  "github": {
    "silent": true
  }
}
